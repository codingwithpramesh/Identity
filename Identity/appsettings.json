{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=Identity Server;Integrated Security=True; TrustServerCertificate=true"
  },
  "Application": {
    "LoginPath": "/Account/login",
    "AppDomain": "https://localhost:7117/",
    "EmailConfirmation": "confirm-email?uid={0}&token={1}",
    "ForgotPassword": "reset-password?uid={0}&token={1}"
  },

 
}
